@model  Chipper.Models.Chip

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Chipper</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Content/start_page_style.css">
</head>
<body class="body">

    <!-- <div style="width:100%;">
        <img src="~/Content/preloader.gif" class="spinner" id="spin" />
        </div> -->
    
    <div class="content">
        <p id="chipper">Chipper</p>
        <div >
            @using (Html.BeginForm("StartPage", "Home", FormMethod.Post))
            {
                @Html.AntiForgeryToken()
                <div class="input-group col-lg-6 col-md-6 col-sm-6 col-md-offset-3 col-sm-offset-3 ">
                    @Html.TextBox("request", null, new { @class= "form-control" })
                    <div class="input-group-btn">
                        <button class="btn btn-default onclick=Spinner()" type="submit">
                            <i class="glyphicon glyphicon-search"></i>
                        </button>
                    </div>
                </div>

            }
           

        </div>
    </div>


</body>
</html>
